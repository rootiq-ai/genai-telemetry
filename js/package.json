{
  "name": "genai-telemetry",
  "version": "1.0.3",
  "description": "OpenTelemetry-based telemetry SDK for Generative AI applications - JavaScript/TypeScript",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./exporters": {
      "require": "./dist/exporters/index.js",
      "import": "./dist/exporters/index.mjs",
      "types": "./dist/exporters/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "genai",
    "llm",
    "telemetry",
    "observability",
    "tracing",
    "opentelemetry",
    "splunk",
    "elasticsearch",
    "datadog",
    "prometheus",
    "grafana",
    "loki",
    "cloudwatch",
    "openai",
    "anthropic",
    "langchain",
    "rag",
    "ai",
    "machine-learning"
  ],
  "author": "Kamal Singh Bisht",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genai-telemetry/genai-telemetry.git"
  },
  "bugs": {
    "url": "https://github.com/genai-telemetry/genai-telemetry/issues"
  },
  "homepage": "https://github.com/genai-telemetry/genai-telemetry#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "optionalDependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.0.0"
  }
}
